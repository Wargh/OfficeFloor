<woof>

	<templates>
		<template path="/child" location="example/Child.ofp">
			<extends template="/parent" />
			<output name="OUTPUT_CHILD" />
		</template>
		<template path="/grandparent" location="example/GrandParent.ofp">
			<output name="OUTPUT_GRAND_PARENT" />
		</template>
		<template path="/parent" location="example/Parent.ofp">
			<extends template="/grandparent" />
			<output name="OUTPUT_PARENT" />
		</template>
	</templates>

</woof>
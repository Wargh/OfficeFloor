<woof>

	<application-paths>
		<application-path path="/applicationPath">
			<section name="SECTION" input="INPUT" />
			<security name="SECURITY" />
			<resource path="/resource.html" />
			<redirect path="/applicationPathLink" />
		</application-path>
		<application-path path="/applicationPathLink">
			<redirect path="/applicationPath" />
		</application-path>
	</application-paths>

	<templates>
		<template path="/templateChild" location="example/Child.ofp" />
		<template path="/templateLink" location="example/Link.ofp">
			<output name="OUTPUT" />
		</template>
	</templates>

	<sections>
		<section name="SECTION" source="net.example.ExampleSectionSource"
			location="SECTION_LOCATION">
			<input name="INPUT" />
			<output name="OUTPUT_A">
				<section name="SECTION_LINK" input="INPUT" />
			</output>
			<output name="OUTPUT_B" />
			<output name="OUTPUT_C">
				<security name="SECURITY" />
			</output>
			<output name="OUTPUT_D">
				<resource path="/resource.html" />
			</output>
			<output name="OUTPUT_E">
				<redirect path="/applicationPath" />
			</output>
		</section>
		<section name="SECTION_LINK" source="net.example.LinkSectionSource"
			location="LINK_LOCATION">
			<input name="INPUT" />
			<output name="OUTPUT">
				<section name="SECTION" input="INPUT" />
			</output>
		</section>
	</sections>

	<securities>
		<security name="SECURITY" source="net.example.HttpSecuritySource">
			<output name="OUTPUT_a">
				<section name="SECTION" input="INPUT" />
			</output>
			<output name="OUTPUT_b" />
			<output name="OUTPUT_c">
				<security name="SECURITY_LINK" />
			</output>
			<output name="OUTPUT_d">
				<resource path="/resource.html" />
			</output>
			<output name="OUTPUT_e">
				<redirect path="/applicationPath" />
			</output>
		</security>
		<security name="SECURITY_LINK" source="net.another.HttpSecuritySource">
			<output name="OUTPUT">
				<security name="SECURITY" />
			</output>
		</security>
	</securities>

	<resources>
		<resource path="/resource.html" />
	</resources>

	<exceptions>
		<exception class="java.lang.Exception">
			<section name="SECTION" input="INPUT" />
			<security name="SECURITY" />
			<resource path="/resource.html" />
			<redirect path="/applicationPath" />
		</exception>
	</exceptions>

	<starts>
		<start>
			<section name="SECTION" input="INPUT" />
		</start>
	</starts>

</woof>
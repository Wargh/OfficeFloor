<woof>

	<application-paths>
		<application-path path="/path" />
	</application-paths>

	<templates>
		<template application-path="template" location="example/Template.ofp"
			class="net.example.LogicClass">
			<extension
				class="net.officefloor.model.woof.MockNoChangeWoofTemplateExtensionSource" />
			<extension
				class="net.officefloor.model.woof.MockChangeWoofTemplateExtensionSource">
				<property name="ONE" value="A" />
				<property name="TWO" value="B" />
			</extension>
		</template>
	</templates>

	<sections />

	<governances>
		<governance name="GOVERNANCE" source="net.example.ExampleGovernanceSource" />
	</governances>

	<securities>
		<security name="SECURITY" source="net.example.HttpSecuritySource" />
	</securities>

	<resources>
		<resource path="Example.html" />
	</resources>

	<exceptions>
		<exception class="java.lang.Exception" />
	</exceptions>

	<starts>
		<start />
	</starts>

</woof>
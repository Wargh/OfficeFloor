<woof>

	<application-paths>
		<application-path path="/pathA" secure="true">
			<service method="GET" />
			<service method="POST" />
			<section name="SECTION_A" input="INPUT_A" />
		</application-path>
		<application-path path="/pathB">
			<template path="/template/{param}" />
		</application-path>
		<application-path path="/pathC">
			<security name="SECURITY_ONE" />
		</application-path>
		<application-path path="/pathD">
			<resource path="/resourceA.html" />
		</application-path>
		<application-path path="/pathE">
			<redirect path="/pathA" />
		</application-path>
	</application-paths>

	<templates>
		<template path="/template/{param}" location="WOOF/TemplateA.ofp"
			class="net.officefloor.plugin.woof.WoofLoaderTest$Template"
			redirect-values-function="redirect" content-type="text/html; charset=UTF-8"
			charset="UTF-8" link-character-seprator="_" secure="true" x="20" y="20">
			<output name="OUTPUT_1" argument-type="java.lang.Integer">
				<section name="SECTION_A" input="INPUT_A" />
			</output>
			<output name="OUTPUT_2">
				<template path="/template/another" />
			</output>
			<output name="OUTPUT_3">
				<security name="SECURITY" />
			</output>
			<output name="OUTPUT_4">
				<resource path="/resource.html" />
			</output>
			<output name="OUTPUT_5" argument-type="java.lang.String">
				<redirect path="/pathA" />
			</output>
			<output name="OUTPUT_6" />
			<link name="LINK_1" secure="true" />
			<link name="LINK_2" secure="false" />
			<render method="POST" />
			<render method="PUT" />
		</template>
		<template path="/template/another" location="WOOF/TemplateB.ofp" />
	</templates>

	<sections>
		<section name="SECTION_A"
			source="net.officefloor.plugin.section.clazz.ClassSectionSource"
			location="net.officefloor.plugin.woof.WoofLoaderTest$Section" x="80"
			y="80">
			<property name="name.one" value="value.one" />
			<property name="name.two" value="value.two" />
			<input name="INPUT_A" parameter-type="java.lang.Integer" />
			<input name="INPUT_B" uri="example" />
			<output name="OUTPUT_A" argument-type="java.lang.String">
				<input section="SECTION_B" input="INPUT_1" />
			</output>
			<output name="OUTPUT_B">
				<template path="/template/{param}" />
			</output>
			<output name="OUTPUT_C">
				<security name="SECURITY_ONE" />
			</output>
			<output name="OUTPUT_D">
				<resource path="/resource.png" />
			</output>
			<output name="OUTPUT_E" argument-type="java.lang.Long">
				<redirect path="/pathC" />
			</output>
			<output name="OUTPUT_F" />
		</section>
		<section name="SECTION_B" source="CLASS"
			location="net.officefloor.plugin.woof.WoofLoaderTest$Section">
			<input name="INPUT_1" />
		</section>
	</sections>

	<securities>
		<security name="SECURITY_ONE"
			source="net.officefloor.plugin.web.http.security.scheme.MockHttpSecuritySource"
			timeout="2000">
			<property name="name.first" value="value.first" />
			<property name="name.second" value="value.second" />
			<content type="application/json" />
			<content type="application/xml" />
			<output name="OUTPUT_ONE">
				<section name="SECTION_B" input="INPUT_1" />
			</output>
			<output name="OUTPUT_TWO">
				<template path="/template/another" />
			</output>
			<output name="OUTPUT_THREE">
				<security name="SECURITY_TWO" />
			</output>
			<output name="OUTPUT_FOUR">
				<resource path="/resource.html" />
			</output>
			<output name="OUTPUT_FIVE">
				<redirect path="/pathD" />
			</output>
			<output name="OUTPUT_SIX" />
		</security>
		<security name="SECURITY_TWO"
			source="net.officefloor.web.http.security.scheme.MockHttpSecuritySource" />
	</securities>

	<resources>
		<resource path="/resource.html" />
		<resource path="/resource.png" />
	</resources>

	<governances>
		<governance name="GOVERNANCE_A"
			source="net.officefloor.plugin.governance.clazz.ClassGovernanceSource">
			<property name="name.a" value="value.a" />
			<property name="name.b" value="value.b" />
			<governance-area x="10" y="10" width="30" height="30" />
			<governance-area x="70" y="70" width="90" height="90" />
		</governance>
		<governance name="GOVERNANCE_B" source="CLASS" />
	</governances>

	<exceptions>
		<exception class="java.lang.Exception">
			<section name="SECTION_A" input="INPUT_A" />
		</exception>
		<exception class="java.lang.RuntimeException">
			<template path="/template/{param}" />
		</exception>
		<exception class="java.lang.UnsupportedOperationException">
			<security name="SECURITY_TWO" />
		</exception>
		<exception class="java.sql.SQLException">
			<resource path="/resource.png" />
		</exception>
		<exception class="java.lang.NullPointerException">
			<redirect path="/pathE" />
		</exception>
		<exception class="java.io.IOException" />
	</exceptions>

	<starts>
		<start>
			<section name="SECTION_A" input="INPUT_A" />
		</start>
		<start>
			<section name="SECTION_B" input="INPUT_1" />
		</start>
	</starts>

</woof>
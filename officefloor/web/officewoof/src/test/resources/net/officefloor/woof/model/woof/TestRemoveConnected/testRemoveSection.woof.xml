<woof>

	<application-paths>
		<application-path path="/applicationPath">
			<template path="/template" />
			<security name="SECURITY" />
			<resource path="/resource.html" />
			<redirect path="/applicationPathLink" />
		</application-path>
		<application-path path="/applicationPathLink">
			<redirect path="/applicationPath" />
		</application-path>
	</application-paths>

	<templates>
		<template application-path="/template" location="example/Template.ofp"
			class="net.example.LogicClass">
			<extends template="/templateLink" />
			<output name="OUTPUT_1" />
			<output name="OUTPUT_2">
				<template path="/templateLink" />
			</output>
			<output name="OUTPUT_3">
				<security name="SECURITY" />
			</output>
			<output name="OUTPUT_4">
				<resource path="/resource.html" />
			</output>
			<output name="OUTPUT_5">
				<redirect path="/applicationPath" />
			</output>
		</template>
		<template application-path="/templateChild" location="example/Child.ofp">
			<extends template="/template" />
		</template>
		<template application-path="/templateLink" location="example/Link.ofp">
			<output name="OUTPUT">
				<template path="/template" />
			</output>
		</template>
	</templates>

	<sections>
		<section name="SECTION_LINK" source="net.example.LinkSectionSource"
			location="LINK_LOCATION">
			<input name="INPUT" />
			<output name="OUTPUT" />
		</section>
	</sections>

	<securities>
		<security name="SECURITY" source="net.example.HttpSecuritySource">
			<output name="OUTPUT_a" />
			<output name="OUTPUT_b">
				<template path="/template" />
			</output>
			<output name="OUTPUT_c">
				<security name="SECURITY_LINK" />
			</output>
			<output name="OUTPUT_d">
				<resource path="/resource.html" />
			</output>
			<output name="OUTPUT_e">
				<redirect path="/applicationPath" />
			</output>
		</security>
		<security name="SECURITY_LINK" source="net.another.HttpSecuritySource">
			<output name="OUTPUT">
				<security name="SECURITY" />
			</output>
		</security>
	</securities>

	<resources>
		<resource path="/resource.html" />
	</resources>

	<exceptions>
		<exception class="java.lang.Exception">
			<template path="/template" />
			<security name="SECURITY" />
			<resource path="/resource.html" />
			<redirect path="/applicationPath" />
		</exception>
	</exceptions>

	<starts>
		<start />
	</starts>

</woof>
<woof>

	<http-continuations>
		<http-continuation path="/pathA" secure="true" x="100"
			y="1001">
			<section name="SECTION_A" input="INPUT_A" />
			<template path="/templateB" />
			<security name="SECURITY_A" />
			<resource path="/resourceA.html" />
			<redirect path="/pathB" />
		</http-continuation>
		<http-continuation path="/pathB" x="110" y="111" />
	</http-continuations>

	<http-inputs>
		<http-input path="/pathC" method="POST" secure="true" x="200"
			y="201">
			<section name="SECTION_B" input="INPUT_0" />
			<template path="/templateA" />
			<security name="SECURITY_B" />
			<resource path="/resourceB.png" />
			<redirect path="/pathA" />
		</http-input>
		<http-input path="/pathD" method="PUT" x="210" y="211" />
	</http-inputs>

	<templates>
		<template path="/templateA" secure="true" location="example/TemplateA.ofp"
			class="net.example.ExampleClassA" content-type="text/plain; charset=UTF-16"
			charset="UTF-16" redirect-values-function="redirect"
			link-separator-character="_" x="300" y="301">
			<extends template="/templateB" />
			<output name="OUTPUT_0" argument-type="java.lang.Integer">
				<section name="SECTION_A" input="INPUT_A" />
			</output>
			<output name="OUTPUT_1">
				<template path="/templateB" />
			</output>
			<output name="OUTPUT_2">
				<resource path="/resourceA.html" />
			</output>
			<output name="OUTPUT_3">
				<security name="SECURITY_B" />
			</output>
			<output name="OUTPUT_4">
				<redirect path="/pathA" />
			</output>
			<output name="OUTPUT_5" />
			<link name="LINK_0" secure="true" />
			<link name="LINK_1" secure="false" />
			<render method="POST" />
			<render method="PUT" />
			<extension class="EXTENSION">
				<property name="NAME.0" value="VALUE.0" />
				<property name="NAME.1" value="VALUE.1" />
			</extension>
			<extension class="net.example.Extension" />
		</template>
		<template path="/templateB" location="example/TemplateB.ofp"
			x="302" y="303" />
	</templates>

	<sections>
		<section name="SECTION_A" source="SECTION" location="SECTION_LOCATION"
			x="400" y="401">
			<property name="name.one" value="value.one" />
			<property name="name.two" value="value.two" />
			<input name="INPUT_A" parameter-type="java.lang.Integer" />
			<input name="INPUT_B" uri="example" />
			<output name="OUTPUT_A" argument-type="java.lang.String">
				<section name="SECTION_B" input="INPUT_0" />
			</output>
			<output name="OUTPUT_B">
				<template path="/templateA" />
			</output>
			<output name="OUTPUT_C">
				<resource path="/resourceB.png" />
			</output>
			<output name="OUTPUT_D">
				<security name="SECURITY_A" />
			</output>
			<output name="OUTPUT_E">
				<redirect path="/pathB" />
			</output>
			<output name="OUTPUT_F" />
		</section>
		<section name="SECTION_B" source="net.example.ExampleSectionSource"
			location="EXAMPLE_LOCATION" x="402" y="403">
			<input name="INPUT_0" />
		</section>
	</sections>

	<securities>
		<security name="SECURITY_A" source="net.example.HttpSecuritySource"
			timeout="2000" x="500" y="501">
			<property name="name.first" value="value.first" />
			<property name="name.second" value="value.second" />
			<content type="application/json" />
			<content type="application/xml" />
			<output name="OUTPUT_ZERO" argument-type="java.lang.String">
				<section name="SECTION_B" input="INPUT_1" />
			</output>
			<output name="OUTPUT_ONE">
				<template path="/templateA" />
			</output>
			<output name="OUTPUT_TWO">
				<resource path="/resourceB.png" />
			</output>
			<output name="OUTPUT_THREE">
				<security name="SECURITY_B" />
			</output>
			<output name="OUTPUT_FOUR">
				<redirect path="/pathA" />
			</output>
			<output name="OUTPUT_FIVE" />
		</security>
		<security name="SECURITY_B" source="net.example.AnotherHttpSecuritySource"
			x="510" y="511" />
	</securities>

	<governances>
		<governance name="GOVERNANCE_A" source="net.example.ExampleGovernanceSource"
			x="600" y="601">
			<property name="name.a" value="value.a" />
			<property name="name.b" value="value.b" />
			<governance-area x="620" y="621" width="640" height="641" />
			<governance-area x="630" y="631" width="650" height="651" />
		</governance>
		<governance name="GOVERNANCE_B" source="net.example.AnotherGovernanceSource"
			x="610" y="611" />
	</governances>

	<resources>
		<resource path="/resourceA.html" x="700" y="701" />
		<resource path="/resourceB.png" x="710" y="711" />
	</resources>

	<exceptions>
		<exception class="java.lang.Exception" x="800" y="801">
			<section name="SECTION_A" input="INPUT_A" />
		</exception>
		<exception class="java.lang.RuntimeException" x="802" y="803">
			<template path="/templateA" />
		</exception>
		<exception class="java.sql.SQLException" x="804" y="805">
			<resource path="/resourceB.png" />
		</exception>
		<exception class="net.example.AuthException" x="806" y="807">
			<security name="SECURITY_B" />
		</exception>
		<exception class="java.lang.NullPointerException" x="808" y="809">
			<redirect path="/pathB" />
		</exception>
		<exception class="java.io.IOException" x="810" y="811" />
	</exceptions>

	<starts>
		<start x="900" y="901">
			<section name="SECTION_A" input="INPUT_A" />
		</start>
		<start x="910" y="911" />
	</starts>

</woof>
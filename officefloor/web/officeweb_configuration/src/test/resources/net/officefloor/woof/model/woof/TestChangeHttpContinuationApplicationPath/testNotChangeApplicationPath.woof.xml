<woof>

	<http-continuations>
		<http-continuation path="/path" />
		<http-continuation path="/pathLink">
			<redirect path="/path" />
		</http-continuation>
	</http-continuations>

	<http-inputs>
		<http-input path="/input" method="POST">
			<redirect path="/path" />
		</http-input>
	</http-inputs>

	<templates>
		<template path="/template" location="example/Template.ofp">
			<output name="OUTPUT">
				<redirect path="/path" />
			</output>
		</template>
	</templates>

	<sections>
		<section name="SECTION" source="net.example.ExampleSectionSource"
			location="SECTION_LOCATION">
			<output name="OUTPUT">
				<redirect path="/path" />
			</output>
		</section>
	</sections>

	<securities>
		<security name="SECURITY" source="net.example.HttpSecuritySource">
			<output name="OUTPUT">
				<redirect path="/path" />
			</output>
		</security>
	</securities>

	<exceptions>
		<exception class="java.io.IOException">
			<redirect path="/path" />
		</exception>
	</exceptions>

</woof>

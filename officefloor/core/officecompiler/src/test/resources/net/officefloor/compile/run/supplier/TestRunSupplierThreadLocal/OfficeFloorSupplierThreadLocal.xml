<context>
	<office-floor auto-wire-objects="true">
		<suppliers>
			<supplier name="SUPPLIER"
				source="${testcase}$MockSupplierSource" />
		</suppliers>
		<managed-object-sources>
			<managed-object-source
				name="MOCK_OBJECT_SOURCE" source="CLASS">
				<properties>
					<property name="class.name" value="${testcase}$MockObject" />
				</properties>
				<managing-office name="OFFICE" />
			</managed-object-source>
		</managed-object-sources>
		<managed-objects>
			<managed-object name="MOCK_OBJECT" scope="PROCESS">
				<managed-object-source
					name="MOCK_OBJECT_SOURCE" />
			</managed-object>
		</managed-objects>
		<offices>
			<office name="OFFICE" source="OFFICE" location="office">
				<object name="MOCK_OBJECT">
					<managed-object name="MOCK_OBJECT" />
				</object>
				<object name="SUPPLIED_OBJECT" />
			</office>
		</offices>
	</office-floor>
	<office>
		<external-managed-objects>
			<external-managed-object name="MOCK_OBJECT"
				object-type="${testcase}$MockObject" object-scope="PROCESS" />
			<external-managed-object
				name="SUPPLIED_OBJECT"
				object-type="${testcase}$MockManagedObjectSource"
				object-scope="PROCESS" />
		</external-managed-objects>
		<section name="SECTION" source="CLASS"
			location="${testcase}$CompileSection">
			<object name="${testcase}$MockObject"
				object-type="${testcase}$MockObject">
				<external-managed-object name="MOCK_OBJECT" />
			</object>
			<object name="${testcase}$MockManagedObjectSource"
				object-type="${testcase}$MockManagedObjectSource">
				<external-managed-object
					name="SUPPLIED_OBJECT" />
			</object>
		</section>
	</office>
</context>
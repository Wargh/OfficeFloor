<context>
	<office-floor>
		<offices>
			<office name="OFFICE" source="OFFICE" location="office" />
		</offices>
	</office-floor>
	<office>
		<sections>
			<section name="SECTION" source="SECTION" location="section" />
		</sections>
	</office>
	<section>
		<function-namespaces>
			<function-namespace name="NAMESPACE" source="CLASS">
				<properties>
					<property name="class.name" value="${testcase}$CompileFunctionClass" />
				</properties>
				<managed-function name="flowFunction" />
			</function-namespace>
		</function-namespaces>
		<functions>
			<function name="FUNCTION" function-namespace="NAMESPACE"
				managed-function="flowFunction" />
			<function-flow name="flow" argument-type="java.lang.String" />
		</functions>
	</section>
</context>
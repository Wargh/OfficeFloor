<context>
	<office-floor>
		<offices>
			<office name="OFFICE" source="OFFICE" location="office" />
		</offices>
	</office-floor>
	<office>
		<sections>
			<section name="SECTION" source="SECTION" location="parent-section" />
		</sections>
	</office>
	<parent-section>
		<section>
			<section>
				<sub-sections>
					<sub-section name="SUB_SECTION" source="SECTION"
						location="sub-section" />
				</sub-sections>
			</section>
		</section>
	</parent-section>
	<sub-section>
		<section>
			<sub-sections>
				<sub-section name="OVERRIDE_SUB_SECTION" source="${testcase}$TestSectionSource"
					location="section">
					<properties>
						<property name="value" value="to be overridden" />
					</properties>
				</sub-section>
			</sub-sections>
		</section>
	</sub-section>
</context>

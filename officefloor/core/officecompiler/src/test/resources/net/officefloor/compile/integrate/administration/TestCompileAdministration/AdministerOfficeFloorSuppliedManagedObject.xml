<context>
	<office-floor>
		<suppliers>
			<supplier name="SUPPLIER"
				source="${testcase}$MockSupplierSource" />
		</suppliers>
		<managed-object-sources>
			<managed-object-source
				name="MANAGED_OBJECT_SOURCE">
				<supplier name="SUPPLIER"
					type="${testcase}$SimpleManagedObject" />
				<managing-office name="OFFICE" />
			</managed-object-source>
		</managed-object-sources>
		<managed-objects>
			<managed-object name="MANAGED_OBJECT" scope="THREAD">
				<managed-object-source
					name="MANAGED_OBJECT_SOURCE" />
			</managed-object>
		</managed-objects>
		<offices>
			<office name="OFFICE" source="OFFICE" location="office">
				<object name="OBJECT">
					<managed-object name="MANAGED_OBJECT" />
				</object>
			</office>
		</offices>
	</office-floor>
	<office>
		<external-managed-objects>
			<external-managed-object name="OBJECT"
				object-type="${testcase}$SimpleManagedObject">
				<administer administration="ADMIN" order="1" />
			</external-managed-object>
		</external-managed-objects>
		<administrations>
			<administration name="ADMIN" source="CLASS">
				<properties>
					<property name="class.name"
						value="${testcase}$SimpleAdmin" />
				</properties>
			</administration>
		</administrations>
		<sections>
			<section name="SECTION" source="SECTION" location="section">
				<sub-section>
					<function name="FUNCTION">
						<pre-administer administration="ADMIN" />
					</function>
				</sub-section>
			</section>
		</sections>
	</office>
	<section>
		<function-namespaces>
			<function-namespace name="NAMESPACE"
				source="CLASS">
				<properties>
					<property name="class.name"
						value="${testcase}$SimpleClass" />
				</properties>
				<managed-function name="function" />
			</function-namespace>
		</function-namespaces>
		<functions>
			<function name="FUNCTION" function-namespace="NAMESPACE"
				managed-function="function" public="false" />
		</functions>
	</section>
</context>

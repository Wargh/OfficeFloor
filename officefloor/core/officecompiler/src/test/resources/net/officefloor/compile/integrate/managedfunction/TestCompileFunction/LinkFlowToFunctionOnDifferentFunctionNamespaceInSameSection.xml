<context>
	<office-floor>
		<offices>
			<office name="OFFICE" source="OFFICE" location="office" />
		</offices>
	</office-floor>
	<office>
		<sections>
			<section name="SECTION" source="SECTION" location="section" />
		</sections>
	</office>
	<section>
		<function-namespaces>
			<function-namespace name="NAMESPACE_A" source="CLASS">
				<properties>
					<property name="class.name" value="${testcase}$CompileFunctionClass" />
				</properties>
				<managed-function name="flowFunction" />
			</function-namespace>
			<function-namespace name="NAMESPACE_B" source="CLASS">
				<properties>
					<property name="class.name" value="${testcase}$CompileFunctionClass" />
				</properties>
				<managed-function name="simpleFunction" />
			</function-namespace>
		</function-namespaces>
		<functions>
			<function name="FUNCTION_A" function-namespace="NAMESPACE_A"
				managed-function="flowFunction">
				<function-flow name="flow">
					<function name="FUNCTION_B" spawn-thread-state="false" />
				</function-flow>
			</function>
			<function name="FUNCTION_B" function-namespace="NAMESPACE_B"
				managed-function="simpleFunction" />
		</functions>
	</section>
</context>
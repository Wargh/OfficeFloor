<context>
	<office-floor>
		<offices>
			<office name="OFFICE" source="OFFICE" location="office" />
		</offices>
	</office-floor>
	<office>
		<managed-object-sources>
			<managed-object-source name="DEPENDENT_SOURCE"
				source="${testcase}$FunctionDependencyManagedObject">
				<function-dependency name="DEPENDENCY">
					<managed-object name="SIMPLE" />
				</function-dependency>
			</managed-object-source>
			<managed-object-source name="SIMPLE_SOURCE"
				source="CLASS">
				<properties>
					<property name="class.name"
						value="${testcase}$SimpleManagedObject" />
				</properties>
			</managed-object-source>
		</managed-object-sources>
		<managed-objects>
			<managed-object name="SIMPLE" scope="PROCESS">
				<managed-object-source name="SIMPLE_SOURCE" />
			</managed-object>
		</managed-objects>
	</office>
</context>
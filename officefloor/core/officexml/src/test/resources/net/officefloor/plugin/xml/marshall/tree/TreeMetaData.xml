<marshall xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="net.officefloor.plugin.xml.marshal"
	xsi:schemaLocation="net.officefloor.plugin.xml.marshal ..\..\..\..\..\..\..\..\src\schema\TreeXmlMarshaller.xsd "
	type="net.officefloor.plugin.xml.marshall.tree.objects.RootObject"
	element="root">
	<attributes>
		<attribute method="getInt" attribute="int" />
	</attributes>
	<object method="getChild" element="child">
		<attributes>
			<attribute method="getLong" attribute="long" />
		</attributes>
		<value method="getFloat" element="float" />
	</object>
	<object method="getChild" element="another-child">
		<attributes>
			<attribute method="getDouble" attribute="double" />
		</attributes>
	</object>
</marshall>

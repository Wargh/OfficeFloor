<?xml version="1.0" encoding="UTF-8"?>
<plugin>

	<extension point="org.eclipse.ui.newWizards">
		<wizard
        category="net.officefloor.category"
        class="net.officefloor.eclipse.woof.WoofNewWizard"
        icon="icons/OfficeFloor.png"
        id="net.officefloor.eclipse.woof.WoofNewWizard"
        name="WoOF"/>
	</extension>
	<extension point="org.eclipse.ui.editors"
		id="net.officefloor.ui.OfficeFloorEditors" 
		name="OfficeFloor Editors"
		>
		<editor
        class="net.officefloor.eclipse.woof.WoofEditPart"
        contributorClass="net.officefloor.eclipse.ide.editor.IdeEditorActionBarContributor"
        default="true"
        extensions="woof"
        icon="icons/OfficeFloor.png"
        id="net.officefloor.eclipse.woof"
        name="WoOF Editor"/>
	</extension>

	<extension point="org.eclipse.ui.preferencePages">
		<page 
			id="new.officefloor.eclipse.ide.preferences"
	    	class="net.officefloor.eclipse.ide.preferences.OfficeFloorIdePreferencePage"
            name="OfficeFloor"
            />
	</extension>
	<extension point="org.eclipse.ui.newWizards">
		<category id="net.officefloor.category" name="OfficeFloor" />
	</extension>

   <extension
         id="net.officefloor.eclipse.configurer.test.fragment"
         point="org.eclipse.e4.workbench.model">
      <fragment
            uri="fragment.e4xmi">
      </fragment>
   </extension>

</plugin>
